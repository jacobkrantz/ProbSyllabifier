{
    "CelexTranscriptionFile": "HMMFiles/DISC/DISC4.txt",
    "NistTranscriptionFile": "HMMFiles/phoneCategoriesArp.txt",
    "comparator": "CELEX",
    "dataLoader": {
        "databaseContext": "wordformsDB"
    },
    "write_results_to_DB": false,
    "wordformsDB": {
        "default_alphabet": "DISC",
        "path": "../wordforms.db",
        "protected_tables": [
            "words",
            "pronunciations",
            "syllabifications"
        ],
        "read_permissions": true,
        "write_permissions": true
    },
    "GeneticAlgorithm": {
      "MaxThreadCount": 5,
      "InitialPopulationSize": 50,
      "PopulationSize": 32,
      "NumMatingPairs": 8,
      "MutationFactor": 0.05,
      "NumEvolutions": 200,
      "NumCategories": 12,
      "TrainingSizeHMM": 3000,
      "TestingSizeHMM": 500,
      "GeneList": [ "m", "F", "n", "N", "C",
                    "H", "p", "b", "t", "d",
                    "k", "g", "J", "_", "f",
                    "v", "T", "D", "s", "z",
                    "S", "Z", "x", "h", "j",
                    "w", "r", "R", "l", "P",
                    "6", "7", "i", "u", "I",
                    "U", "1", "5", "@", "E",
                    "3", "V", "$", "9", "4",
                    "8", "{", "c", "#", "Q",
                    "2", "q", "0", "~"],
      "LogFileLocation": "GeneticAlgorithm/EvolutionLogs/"
    }
}
