{
    "CelexTranscriptionFile": "HMMFiles/DISC/DISC4.txt",
    "NistTranscriptionFile": "HMMFiles/phoneCategoriesArp.txt",
    "SQLite3": {
        "affinities": [
            "TEXT",
            "NUMERIC",
            "INTEGER",
            "REAL",
            "NONE"
        ],
        "datatypes": [
            "NULL",
            "INTEGER",
            "REAL",
            "TEXT",
            "BLOB"
        ]
    },
    "comparator": "CELEX",
    "dataLoader": {
        "databaseContext": "wordformsDB"
    },
    "write_results_to_DB": false,
    "wordformsDB": {
        "check_same_thread": false,
        "default_alphabet": "DISC",
        "execute_parameters": "PRAGMA journal_mode = WAL",
        "path": "../wordforms.db",
        "protected_tables": [
            "words",
            "pronunciations",
            "syllabifications"
        ],
        "read_permissions": true,
        "sources": [
            "webCELEX"
        ],
        "write_permissions": true
    },
    "GeneticAlgorithm": {
      "InitialPopulationSize": 50,
      "PopulationSize": 32,
      "NumMatingPairs": 8,
      "MutationFactor": 0.05,
      "NumEvolutions": 20,
      "NumCategories": 12,
      "TrainingSizeHMM":3000,
      "TestingSizeHMM": 500,
      "GeneList": [ "m", "F", "n", "N", "C",
                    "H", "p", "b", "t", "d",
                    "k", "g", "J", "_", "f",
                    "v", "T", "D", "s", "z",
                    "S", "Z", "x", "h", "j",
                    "w", "r", "R", "l", "P",
                    "6", "7", "i", "u", "I",
                    "U", "1", "5", "@", "E",
                    "3", "V", "$", "9", "4",
                    "8", "{", "c", "#", "Q",
                    "2", "q", "0", "~"],
      "LogFileLocation": "GeneticAlgorithm/EvolutionLogs/"
    }
}
