{
  "environment" : "Local",
  "CelexTranscriptionFile": "HMMFiles/DISC/generated/trans_evo_1746.txt",
  "NistTranscriptionFile": "HMMFiles/phoneCategoriesArp.txt",
  "comparator": "CELEX",
  "NGramValue": 2,
  "logLevel": "warning",
  "dataLoader": {
    "databaseContext": "wordformsDB"
  },
  "model": {
    "use_start_tags": true,
    "k_smoothing": 0.0
  },
  "write_results_to_DB": true,
  "wordformsDB": {
    "default_alphabet": "DISC",
    "path": "../wordforms.db",
    "protected_tables": [
      "words",
      "pronunciations",
      "syllabifications"
    ],
    "read_permissions": true,
    "write_permissions": true
  },
  "crossValidation": {
    "KValue": 10,
    "NValue": 60000
  },
  "GeneticAlgorithm": {
    "MaxThreadCount": 4,
    "InitialPopulationSize": 4,
    "PopulationSize": 4,
    "NumMatingPairs": 1,
    "NumChromsNotToMutate": 1,
    "BaseMutationFactor": 0.040,
    "DesiredDeviation": 1.50,
    "NumChromsInDeviation": 8,
    "NumEvolutions": 10,
    "NumCategories": 15,
    "CategoryRestriction": "True",
    "CategoryRestrictionCount": 3,
    "TrainingSizeHMM": 10000,
    "TestingSizeHMM": 1000,
    "GeneList": [ "m", "F", "n", "N", "C",
                  "H", "p", "b", "t", "d",
                  "k", "g", "J", "_", "f",
                  "v", "T", "D", "s", "z",
                  "S", "Z", "x", "h", "j",
                  "w", "r", "R", "l", "P",
                  "6", "7", "i", "u", "I",
                  "U", "1", "5", "@", "E",
                  "3", "V", "$", "9", "4",
                  "8", "{", "c", "#", "Q",
                  "2", "q", "0", "~"],
    "LogFileLocation": "GeneticAlgorithm/EvolutionLogs/"
  },
  "PhoneOptimize": {
    "phone_list": ["s"],
    "transcription_file": "test.txt",
    "evo_frequency": 10,
    "evo_at_least": 200
  },
  "email": {
    "from_address": "cps.research.krantz@gmail.com",
    "to_addresses": [
      "jkrantz@zagmail.gonzaga.edu","mdulin2@zagmail.gonzaga.edu"
    ],
    "password": "",
    "smtp_server": "smtp.gmail.com",
    "smtp_port": 587
  }
}
