{
  "environment" : "Local",
  "celex_transcription_file": "HMMFiles/DISC/generated/trans_evo_1746.txt",
  "nist_transcription_file": "HMMFiles/phoneCategoriesArp.txt",
  "comparator": "CELEX",
  "hmm_order": 1,
  "log_level": "info",
  "data_loader": {
    "database_context": "wordforms_db"
  },
  "model": {
    "use_start_tags": true,
    "k_smoothing": 0.0
  },
  "write_results_to_db": false,
  "wordforms_db": {
    "default_alphabet": "DISC",
    "path": "../wordforms.db",
    "protected_tables": [
      "words",
      "pronunciations",
      "syllabifications"
    ],
    "read_permissions": true,
    "write_permissions": true
  },
  "cross_validation": {
    "k_value": 10,
    "n_value": 60000
  },
  "genetic_algorithm": {
    "max_thread_count": 8,
    "initial_population_size": 16,
    "population_size": 16,
    "num_mating_pairs": 4,
    "num_chroms_not_to_mutate": 1,
    "base_mutation_factor": 0.040,
    "desired_deviation": 1.50,
    "num_chroms_in_deviation": 8,
    "num_evolutions": 10,
    "num_categories": 15,
    "category_restriction": "True",
    "category_restriction_count": 3,
    "training_size_hmm": 1000,
    "testing_size_hmm": 300,
    "gene_list": [ "m", "F", "n", "N", "C",
                  "H", "p", "b", "t", "d",
                  "k", "g", "J", "_", "f",
                  "v", "T", "D", "s", "z",
                  "S", "Z", "x", "h", "j",
                  "w", "r", "R", "l", "P",
                  "6", "7", "i", "u", "I",
                  "U", "1", "5", "@", "E",
                  "3", "V", "$", "9", "4",
                  "8", "{", "c", "#", "Q",
                  "2", "q", "0", "~"],
    "log_file_location": "GeneticAlgorithm/EvolutionLogs/"
  },
  "phone_optimize": {
    "phone_list": ["s"],
    "transcription_file": "test.txt",
    "evo_frequency": 2,
    "evo_at_least": 0
  },
  "email": {
    "from_address": "cps.research.krantz@gmail.com",
    "to_addresses": [
      "jkrantz@zagmail.gonzaga.edu","mdulin2@zagmail.gonzaga.edu"
    ],
    "password": "",
    "smtp_server": "smtp.gmail.com",
    "smtp_port": 587
  }
}
